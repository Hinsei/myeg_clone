<div id="register">
  <%= form_for @changeset, user_path(@conn, :create), fn f ->  %>
    <div class="form-group">
      <%= label f, "Email" %>
      <%= email_input f, :email, class: "form-control"%>
    </div>

    <div class="form-group">
      <%= label f, "Password" %>
      <%= password_input f, :password, class: "form-control"%>
    </div>

    <div>
      <%= submit "Register" %>
    </div>
  <% end %>
</div>
